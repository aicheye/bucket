# Copy to .env.local and fill the values

# OAuth Credentials
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# NextAuth Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=replace-with-a-random-secret

# GraphQL API Configuration
GRAPHQL_URL=https://your-graphql.example.com/graphql
GRAPHQL_SERVICE_SECRET=replace-with-service-secret

# Telemetry Configuration
TELEMETRY_SECRET=replace-with-telemetry-secret
TELEMETRY_ADMIN_ID=replace-with-admin-user-id

# Cron Job Secret
CRON_SECRET=replace-with-cron-secret
